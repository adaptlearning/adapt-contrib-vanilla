{
  "name": "adapt-contrib-vanilla",
  "version": "6.2.3",
  "framework": ">=5.18.7",
  "homepage": "https://github.com/adaptlearning/adapt-contrib-vanilla",
  "bugs": "https://github.com/adaptlearning/adapt-contrib-vanilla/issues",
  "theme": "vanilla",
  "displayName": "Vanilla",
  "description": "A core bundled theme",
  "main": "js/theme.js",
  "keywords": [
    "adapt-plugin",
    "adapt-theme"
  ],
  "private": true,
  "license": "GPL-3.0",
  "targetAttribute": "_vanilla",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-eslint": "^3.0.9",
    "semantic-release": "^19.0.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "bower.json",
            "package.json"
          ],
          "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "scripts": {
    "postversion": "cp package.json bower.json"
  }
}
